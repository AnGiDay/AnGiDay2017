<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.khach.myrestaurant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".View.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
        android:name=".View.MainDetailRestaurant"
        android:label="@string/title_activity_main_detail_restaurant"
        android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".View.Update_FreeSpace"
            android:label="Cập nhật số chổ trống">
    </activity>
    </application>

</manifest>